@charset "utf-8";
/***公共样式****/
body, div, span, h1, h2, h3, h4, h5, h6, p, em, img, strong, b, small, u, i, center, dl, dt, dd, ol, ul, li,  sub, sup, tt, var, del, dfn,  ins, kbd, q, s, samp,  strike, applet, object, iframe, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, embed, figure, figcaption, footer, header, hgroup, menu, nav, output, ruby, section, summary, time, blockquote, pre, a, abbr, acronym, address, big, cite, code, mark, audio, video, input, textarea, select { margin:0; padding:0;  box-sizing:border-box;}	

ol, ul{list-style:none;}

html { overflow-x:hidden; -webkit-text-size-adjust:none; min-height: 100%}
body { font-size:12px; font-family:Microsoft YaHei,Arial,SimSun; color:#666666; overflow-x:hidden; min-height: 100%;}

/*a 链接*/
a { text-decoration:none; outline:none; }
a:hover{text-decoration: underline;}
/*图片居中*/
.jzimg { display:table-cell; *display:block; overflow:hidden; font:0px/0px 'Arial'; text-align:center; vertical-align:middle;}
fieldset, img { border:0; vertical-align:middle}

/*浮动*/
.fl {float:left; display:inline}
.fr {float:right; display:inline}
.clear { clear:both}
.clearfix { *zoom:1;}
.clearfix:after{clear:both; display:block; height:0; visibility:hidden; line-height:0; content:'\20';}

/* HTML5 重置为较旧版本的浏览器 */
article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section { display:block;}
blockquote, q{quotes:none;}
blockquote:before, blockquote:after, q:before, q:after { content:''; content:none; }
table { border-collapse: collapse; border-spacing: 0; }

/* 按钮样式 */
input, button, textarea, select { font-size:100%; outline:none; resize:none}



/*删除火狐下按钮默认样式*/
input[type="reset"]::-moz-focus-inner,
input[type="button"]::-moz-focus-inner,
input[type="submit"]::-moz-focus-inner,
input[type="file"] > input[type="button"]::-moz-focus-inner{ border:none; padding:0; }


/*字体大小、缩进*/
.ffc{font-family:"微软雅黑";}
.ffe{font-family:Arial;}
.f12 { font-size:12px;}
.f13 { font-size:13px;}
.f14 { font-size:14px;}
.f16 { font-size:16px;}
.f18{ font-size:18px;}
.f20{ font-size:20px;}
.f24{ font-size:24px;}
.t2{text-indent:2em;}

/*段落间距*/
.lh{ line-height:1.5em;}
.ta {text-align:center;}

/*上下间距*/
.mb5 { margin-bottom:5px;}
.mb10{ margin-bottom:10px;}
.mb15{ margin-bottom:15px;}
.mb20{ margin-bottom:20px;}
.mb25{ margin-bottom:25px;}

.mr5 {margin-right:5px;}

/*内敛---块状元素*/
.dy {display:block;}

/*文字排版方向*/
.tar { text-align:right;}
.tac {text-align:center;}
.ws { white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}/*使文字部分超出隱藏*/

/*图片居中显示*/
.jzimg1 img{max-height:100%; max-width:100%; vertical-align:middle;}/*这个为了解决图片放大缩小依旧居中的问题*/
.jzimg1 i{height:100%; vertical-align:middle; display:inline-block;}

/*分页样式*/
.fenye{padding: 15px 0;text-align: center;}
.fenye a{padding: 3px 7px;border: 1px solid #ccc;font-size: 12px;color: #333;}
.fenye a.fenye_on,.fenye a:hover{background: #ED1B23; color: #fff;text-decoration: none;}

/*自动排列display:inline-block;的元素之间的距离，最好用伪类*/
.justfix{display:inline-block;width:100%; height:0; overflow:hidden;content: "\20";}


/*正式样式开始*/
.form_group{
	width: 100%;
	background: #fff;
}

.form_group .hd {
	height: 50px;
	line-height: 50px;
	background: #eee;
}
.form_group .hd ul{
	height: 50px;
}
.form_group .hd li {
	width: 50%;
	float: left;
	text-align: center;
	color: #222;
	font-size: 16px;
	cursor: pointer;
}
.form_group .hd li.on{
	background: #fff;
	border-top: 2px solid #1d9ddc;
}


.form_group .bd{
	margin-top: 10px;
}

.form_group .bd .input_group {
	height: 40px;
	line-height: 40px;
	margin-bottom: 20px;
	padding:0 15px;
}

.form_group .bd .input_group label{
	display: inline-block;
	text-align: right;
	font-size: 14px;
	color: #333;
}

.input{
	height: 35px;
	border:1px solid #ccc;
	background: none;
	padding-left: 10px;
	border-radius: 5px;
	font-family: "Microsoft Yahei",Arial, Helvetica, sans-serif;
	color: #666;
	font-size: 14px;
	transition: all 0.5s ease;
}
.input:focus{
	border-color:#1d9ddc;
	box-shadow: 0 0 5px #1d9ddc;
}
.w100{width: 100%;}
.w90{width: 90%;}
.w90{width: 80%;}
.w70{width: 70%}
.w60{width: 60%}
.w50{width: 50%}
.w40{width: 40%}
.w30{width: 30%}
.w20{width: 20%}
.w10{width: 10%}
.w5{width: 5%}

.form_group .bd .input_group input[type="text"].hyyzm{
	width: 70%;
	border-radius: 5px 0 0 5px;
	border-right: none;
	float: left;
}
.form_group .bd .input_group .hqyzm{
	display: inline-block;
	font-size: 14px;
	margin-left: -0.5%;
	color: #fff;
	background: #f00;
	text-align: center;
	border-radius:0 5px 5px 0;
	line-height: 35px;
	height: 35px;
	border: 0;
}
.form_group .bd .input_group .hqyzm:hover{
	cursor: pointer;
}


.text-center{text-align: center;}
.dlBtn{
    height: 41px;
    border-radius: 4px;
    background: #2795dc;
    border: none;
    cursor: pointer;
    border-bottom: 3px solid #0078b3;
    color: #fff;
    font-size: 16px;
    padding: 0 30px;
    text-align: center;
    outline: none;
    font-family: "Microsoft Yahei",Arial, Helvetica, sans-serif;
}    
.dlBtn:hover{
	background: #0081c1;
    border-bottom: 3px solid #006698;
    color: #fff;
    text-decoration: none;
}

.item .info{
	width: 95%;
	margin: 15px auto 10px auto;
    border: 1px solid #eee;
    border-radius: 3px;
    height: 40px;
    line-height: 40px;
    font-size: 16px;
    border: 1px #CCCCCC solid;
    text-align: center;
}


#captchaform{width:100%; height:100%; position: absolute; top:0; left: 0; text-align: center;

background:rgba(0, 0, 0, 0.95) none repeat scroll 0 0 !important;/*实现FF背景透明，文字不透明*/
filter:Alpha(opacity=95); background:#000;/*实现IE背景透明*/}


.closethis{position: absolute;top:0px; right: 0px; width:50px; height: 50px; z-index: 999; display: none}

.form_group .bd .input_group .sendok,.form_group .bd .input_group .sendok:hover{background: #eee; color:#222; cursor: not-allowed;}

#captchaform{padding:0 10px;}
.hidecode{position:absolute; height: 35px; top: 20%}
.hiddsub{position:absolute; top: 35%; text-align: center; left: 15%}


a.logreg{display: block !important; width: 100 !important%}